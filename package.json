    "gatsby-plugin-google-analytics": "^1.0.20",
    "gatsby-plugin-manifest": "^1.0.15",
    "gatsby-plugin-nprogress": "^1.0.13",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.6",
    "gatsby-plugin-sharp": "^1.6.38",
    "gatsby-plugin-styled-components": "^2.0.9",
    "gatsby-remark-emoji": "^0.0.1",
    "gatsby-source-filesystem": "^1.5.26",
    "gatsby-source-github-api": "^0.0.3",
    "gatsby-transformer-remark": "^1.7.34",
    "gatsby-transformer-sharp": "^1.6.22",
    "jest": "^22.4.2",
    "jest-enzyme": "^5.0.3",
    "lodash": "^4.17.5",
    "mobile-detect": "^1.4.1",
    "moment": "^2.21.0",
    "react-burger-menu": "^2.3.0",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-modal": "^3.3.1",
    "react-motion": "^0.5.2",
    "react-reveal": "^1.1.10",
    "react-share": "^2.0.0",
    "react-slick": "^0.19.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^3.1.6",
    "styled-tools": "^0.4.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "prod": "cross-env NODE_ENV=production gatsby serve -p 9003",
    "build": "cross-env NODE_ENV=production gatsby build",
    "ngrok": "ngrok http 8000",
    "dev": "cross-env NODE_ENV=development gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -o ./public-storybook -c .storybook",
    "deploy-storybook": "storybook-to-ghpages -o ./public-storybook -c .storybook",
    "test": "jest && bundlesize"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bundlesize": "^0.16.0",
    "cross-env": "^5.1.3",
    "jsdom-quokka-plugin": "^1.0.11",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "globaldev@naver.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  }
}
