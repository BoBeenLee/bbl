{
  "name": "bobeenlee",
  "version": "1.0.0",
  "description": "BoBeenLee",
  "main": "n/a",
  "scripts": {
    "start:staging": "GATSBY_ENV=staging gatsby develop",
    "start:production": "GATSBY_ENV=production gatsby develop",
    "build:staging": "GATSBY_ENV=staging gatsby build",
    "build:production": "GATSBY_ENV=production gatsby build",
    "prod": "cross-env NODE_ENV=production gatsby serve -p 9003",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 gatsby build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages -o ./public-storybook -c .storybook",
    "test": "jest && bundlesize",
    "type-check": "tsc --noEmit",
    "prettier": "prettier './src/**/*.{js,ts,tsx}' --write",
    "lint:css": "stylelint './src/**/*.tsx'",
    "eslint": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}'",
    "eslint:fix": "./node_modules/.bin/eslint './src/**/*.{js,ts,tsx}' --fix",
    "prepush": "npm run lint:css && npm run eslint"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "axios": "^0.18.0",
    "cross-env": "^5.1.6",
    "date-fns": "^1.29.0",
    "firebase": "^7.8.0",
    "framer-motion": "^1.8.3",
    "gatsby": "^2.17.13",
    "gatsby-image": "^2.2.32",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-netlify-cms": "^4.1.33",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sharp": "^2.2.38",
    "gatsby-plugin-styled-components": "^3.1.12",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-typescript": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-source-github-api": "^0.0.3",
    "gatsby-transformer-remark": "2.6.48",
    "gatsby-transformer-sharp": "^2.3.4",
    "gsap": "^2.1.3",
    "lodash": "^4.17.10",
    "mobile-detect": "^1.4.1",
    "mobx": "^4.9.4",
    "mobx-react": "^5.4.3",
    "mobx-state-tree": "^3.15.0",
    "moment": "^2.22.1",
    "netlify-cms": "2.10.2",
    "netlify-cms-app": "^2.10.1",
    "qs": "^6.6.0",
    "react": "^16.11.0",
    "react-burger-menu": "^2.6.12",
    "react-dom": "^16.11.0",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.7.0",
    "react-modal": "^3.4.4",
    "react-reveal": "^1.2.2",
    "react-share": "^2.1.1",
    "react-slick": "^0.23.1",
    "recompose": "^0.30.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.123",
    "@types/qs": "^6.5.2",
    "@types/react-burger-menu": "^2.2.5",
    "@types/react-dom": "^16.8.3",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^5.0.8",
    "@types/react-icons": "^3.0.0",
    "@types/react-modal": "^3.10.4",
    "@types/react-share": "^3.0.3",
    "@types/react-slick": "^0.23.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby": "^0.2.21",
    "babel-preset-react-app": "^9.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "3.1.0",
    "jest": "^24.5.0",
    "jest-enzyme": "^6.0.1",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.4.0",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-js": "^3.3.7",
    "stylelint": "12.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "1.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "author": "BoBeenLee <globaldev@naver.com>",
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "globaldev@naver.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run prettier",
      "npm run eslint:fix",
      "git add"
    ]
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  },
  "bundlesize": [
    {
      "path": "./public/*.js",
      "maxSize": "300 kB"
    }
  ]
}
