{
  "name": "BoBeenLee",
  "description": "BoBeenLee",
  "version": "1.0.0",
  "author": "BoBeenLee <globaldev@naver.com>",
  "bundlesize": [
    {
      "path": "./public/*.js",
      "maxSize": "300 kB"
    }
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
