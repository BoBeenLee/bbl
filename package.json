{
  "name": "BoBeenLee",
  "description": "BoBeenLee",
  "version": "1.0.0",
  "author": "BoBeenLee <globaldev@naver.com>",
  "bundlesize": [
    {
      "path": "./public/*.js",
      "maxSize": "300 kB"
    }
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gatsby": "^1.9.261",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-remark-emoji": "^0.0.2",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-source-github-api": "^0.0.3",
    "gatsby-transformer-remark": "^1.7.41",
    "gatsby-transformer-sharp": "^1.6.24",
    "jest": "^22.4.4",
    "jest-enzyme": "^6.0.0",
    "lodash": "^4.17.10",
    "mobile-detect": "^1.4.1",
    "moment": "^2.22.1",
    "react-burger-menu": "^2.4.4",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-modal": "^3.4.4",
    "react-motion": "^0.5.2",
    "react-reveal": "^1.2.2",
    "react-share": "^2.1.1",
    "react-slick": "^0.23.1",
    "react-test-renderer": "^16.3.2",
    "styled-components": "^3.2.6",
    "styled-tools": "^0.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "prod": "cross-env NODE_ENV=production gatsby serve -p 9003",
    "build": "cross-env NODE_ENV=production gatsby build",
    "ngrok": "ngrok http 8000",
    "eslint": "eslint src/ --fix",
    "dev": "cross-env NODE_ENV=development gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -o ./public-storybook -c .storybook",
    "deploy-storybook": "storybook-to-ghpages -o ./public-storybook -c .storybook",
    "test": "jest && bundlesize"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bundlesize": "^0.17.0",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.15.0-rc.13",
    "jsdom-quokka-plugin": "^1.0.13",
    "lint-staged": "^7.1.1",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "storybook-deployer": {
    "gitUsername": "GH Pages Bot",
    "gitEmail": "globaldev@naver.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run eslint -- --fix",
      "git add"
    ]
  }
}
